{
    "compatibilities": [
        "EC2"
    ],
    "containerDefinitions": [
        {
            "cpu": 512,
            "environment": [
                {
                    "name": "EFS_MOUNT_PATH",
                    "value": "/mnt/efs/uploads"
                },
                {
                    "name": "DB_NAME",
                    "value": "sampledb"
                },
                {
                    "name": "JAVA_OPTS",
                    "value": "-Xms512m -Xmx1024m -XX:+UseG1GC"
                }
            ],
            "essential": true,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8080/api/health || exit 1"
                ],
                "interval": 30,
                "retries": 3,
                "startPeriod": 60,
                "timeout": 5
            },
            "image": "304936889031.dkr.ecr.ap-northeast-2.amazonaws.com/sample-spring-app:9bc032c5ed913b137a684c42363f11eaad3575a7",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/sample-app",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "memory": 1024,
            "memoryReservation": 512,
            "mountPoints": [
                {
                    "containerPath": "/mnt/efs/uploads",
                    "readOnly": false,
                    "sourceVolume": "efs-volume"
                }
            ],
            "name": "sample-app",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "secrets": [
                {
                    "name": "DB_HOST",
                    "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:304936889031:secret:test/rds-tDJQVH:host::"
                },
                {
                    "name": "DB_PORT",
                    "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:304936889031:secret:test/rds-tDJQVH:port::"
                },
                {
                    "name": "DB_USERNAME",
                    "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:304936889031:secret:test/rds-tDJQVH:username::"
                },
                {
                    "name": "DB_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:304936889031:secret:test/rds-tDJQVH:password::"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        }
    ],
    "executionRoleArn": "arn:aws:iam::304936889031:role/ecsTaskExecutionRole",
    "family": "sample-task",
    "networkMode": "bridge",
    "placementConstraints": [],
